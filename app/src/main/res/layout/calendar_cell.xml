<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/parentView"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/round_background3"
    android:backgroundTint="@color/seldy4"
    android:layout_margin="3dp"
    >
    <View
        android:id="@+id/calendarcircle2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/round_background3"
        android:backgroundTint="@color/seldy5"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />
    <View
        android:id="@+id/calendarcircle"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/round_background3"
        android:backgroundTint="@color/seldy1"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <View
        android:id="@+id/calendarcircle1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/round_background4"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/cellDayText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1"
        android:textColor="@color/gray"
        android:textSize="12dp"
        android:fontFamily="@font/hancom_gothic_bold"
        android:layout_margin="7dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.25"
        />
    <TextView
        android:id="@+id/cellYearMonthDay_tx"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="@+id/cellDayText"
        app:layout_constraintEnd_toEndOf="@+id/cellDayText"/>
</androidx.constraintlayout.widget.ConstraintLayout>